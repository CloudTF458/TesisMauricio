{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Silver');
  Application.CreateForm(TmoduloDatos, moduloDatos);
  Application.CreateForm(TfmAcceso, fmAcceso);
  Application.CreateForm(TfmPrincipal, fmPrincipal);
  Application.CreateForm(TfmClientesa2, fmClientesa2);
  Application.CreateForm(TfmProveedores, fmProveedores);
  Application.CreateForm(TfmBuscarClientes, fmBuscarClientes);
  Application.CreateForm(TfmModificarClientes, fmModificarClientes);
  Application.CreateForm(TfmTecnicos, fmTecnicos);
  Application.CreateForm(TfmInventario, fmInventario);
  Application.CreateForm(TfmArrendamientos, fmArrendamientos);
  Application.CreateForm(TfmBuscarProductos, fmBuscarProductos);
  Application.CreateForm(TfmModificarInventarios, fmModificarInventarios);
  Application.Run;
end.
